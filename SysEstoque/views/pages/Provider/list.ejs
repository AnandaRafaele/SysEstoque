
<!-- LISTA DE PRODUTOS -->
<div class="container-fuild">
    <div class="card">
            <h4 class="card-header">
                    <i class="fas fa-users"></i> Lista de Produtos</h4>
            <div class="card-body">
                   
                    <table class="table table-responsive-lg" border="1px">
                            <thead class="thead-dark">
                                    <tr>
                                            <!--
                                            <th scope="col">
                                                    <i class="fas fa-id-card-alt"></i> ID</th>
                                            -->
                                        <th scope="col">
                                                <i class="fas fa-user"></i> Nome
                                        </th>
                                            
                                    </tr>
                            </thead>
                            <tbody>
                                    
                                    <% ufs.forEach(function(uf){%>
                                            <tr>
                                                    <!--<th scope="row">1</th>-->
                                                    <td>
                                                            <%= uf.nome %>
                                                    </td>
                                                    <td>     
                                                            <a href="/product/edit/"> Editar</a>
                                                    </td>
                                                    <td>
                                                            <a href="http:/product/delete/>"> Excluir</a>
                                                    </td>
                                            </tr>
                                            <% }) %>
                            </tbody>
                    </table>
            </div>
    </div>

</div>
<!-- FIM DA LISTA DE USUÃRIOS -->

















































